# Runtime environment
SCBDB_ENV=development

# Database
DATABASE_URL=postgres://scbdb:scbdb@localhost:5432/scbdb

# Docker Compose database settings (optional overrides)
POSTGRES_DB=scbdb
POSTGRES_USER=scbdb
POSTGRES_PASSWORD=scbdb
POSTGRES_PORT=5432

# API security
SCBDB_API_KEY_HASH_SALT=change-me

# Server configuration
SCBDB_BIND_ADDR=0.0.0.0:3000
SCBDB_LOG_LEVEL=info

# Config files
SCBDB_BRANDS_PATH=./config/brands.yaml

# External providers
LEGISCAN_API_KEY=

